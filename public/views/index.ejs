<!doctype html>
<html lang='en'>
<head>
  <% include partials/head %>
</head>
<body>
  <% include partials/header %>
  <!-- LOCATIONS VIEW -->
  <div id='view-locations' class='view container'>
    <p data-bind='text: User.name'></p>
    <p data-bind='text: User.created'></p>
    <p data-bind='text: User.id'></p>
    <!-- ko foreach: Data.locationRows -->
    <div class='row' data-bind='foreach: $data'>
      <div class='col-sm-3'>
        <p data-bind='text: name'></p>
      </div>
    </div>
    <!-- /ko -->
  </div><!-- /LOCATIONS VIEW -->

  <!-- MENUS VIEW -->
  <div id='view-menus' class='view container'>
    <p> Menu view </p>
  </div><!-- /MENUS VIEW -->

  <!-- ITEMS VIEW -->
  <div id='view-items' class='view container'>
    <p> Items view </p>
  </div><!-- /ITEMS VIEW -->

  <% include partials/footer %>
</body>
</html>
